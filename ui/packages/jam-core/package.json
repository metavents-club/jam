{
  "name": "jam-core",
  "description": "Jam SDK for creating custom audio spaces like Clubhouse / Twitter Spaces, made with WebRTC",
  "version": "0.3.17",
  "main": "dist/index.js",
  "license": "(AGPL-3.0 OR Apache-2.0)",
  "type": "module",
  "exports": {
    "import": "./dist/index.js"
  },
  "proxy": "../../jam-core.ts",
  "scripts": {
    "build": "tsc --emitDeclarationOnly && mv dist/jam-core.d.ts dist/index.d.ts && node ../buildProxyPackage.mjs",
    "bundle": "esbuild --bundle ../../jam-core.ts --minify --outfile=./dist/bundle.js --format=esm --target=es6",
    "size": "cat dist/bundle.js | gzip | wc -c",
    "prepublishOnly": "yarn build && yarn bundle"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "fast-base64": "^0.1.8",
    "mediasoup-client": "^3.6.31",
    "minimal-state": "^5.6.0",
    "ua-parser-js": "^0.7.24",
    "watsign": "^0.1.8"
  },
  "devDependencies": {
    "typescript": "^4.3.4"
  }
}
